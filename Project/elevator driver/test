void* buttonCheck(void){
	int button_pressed=0;	
	while (1){
		for(int i = 0;i<4;i++){
			for(int j = 0;j<3;j++){
				if(elev_get_button_signal(j,i)==1)	{
					button_pressed = button_channel_matrix[i,j];
					printf("Button: Floor: %d Command: %d\n",i,j);
				}
			}
		}
	}
	return EXIT_SUCCESS; 
}

void goToFloor(int floor){
	if(elev_get_floor_sensor_signal() - floor < 0)
		elev_set_motor_direction(DIRN_UP);
	else if(elev_get_floor_sensor_signal() - floor > 0)
		elev_set_motor_direction(DIRN_DOWN);

	while(elev_get_floor_sensor_signal() != floor-1){
//make some kind of pull function that returns the next floor to go to
		//elev_set_floor_indicator(elev_get_floor_sensor_signal());
	}
	elev_set_motor_direction(DIRN_STOP);
	elev_set_door_open_lamp(1);
}
